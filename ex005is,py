n = input('Digite algo  ')
print('É um número? ')
print(n.isnumeric())
print('É alfanumerico?')
print(n.isalnum())
print('É alfabetico?')
print(n.isalpha())
print('Esta em letras maiusculas?')
print(n.isupper())
print('Esta em letras miinusculas?')
print(n.islower())
print('É um número real?')
print(n.isdecimal())

