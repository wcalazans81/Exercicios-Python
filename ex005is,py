n = input('Digite algo  ')
print('Seu tipo primitivo é: ', type(n))
print('É um número? ',n.isnumeric())
print('É só esppaços? ', n.isspace())
print('É alfanumerico? ', n.isalnum())
print('É alfabetico? ', n.isalpha())
print('Esta em letras maiusculas ?', n.isupper())
print('Esta em letras miinusculas?', n.islower())
print('É um número real?', n.isdecimal())
print('Está capitalizado? ', n.istitle())
